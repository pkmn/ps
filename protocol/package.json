{
  "name": "@pkmn/protocol",
  "version": "0.6.28",
  "description": "Parsing logic for Pok√©mon Showdown's PROTOCOL and SIM-PROTOCOL",
  "repository": "github:pkmn/ps",
  "license": "MIT",
  "sideEffects": false,
  "main": "build/index.js",
  "module": "build/index.mjs",
  "types": "build/index.d.ts",
  "unpkg": "build/index.min.js",
  "exports": {
    ".": {
      "require": "./build/index.js",
      "import": "./build/index.mjs"
    },
    "./verifier": {
      "require": "./build/verifier/index.js",
      "import": "./build/verifier/index.mjs"
    }
  },
  "typesVersions": {
    "*": {
      "verifier": [
        "./build/verifier/index.d.ts"
      ]
    }
  },
  "bin": {
    "generate-handler": "./generate-handler",
    "protocol-verifier": "./protocol-verifier"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "build"
  ],
  "dependencies": {
    "@pkmn/types": "^4.0.0"
  },
  "devDependencies": {
    "@pkmn/dex": "^0.9.22",
    "@pkmn/data": "^0.9.22"
  },
  "scripts": {
    "lint": "eslint --cache src",
    "fix": "eslint --fix src",
    "compile": "tsc -p .",
    "build": "npm run compile && node ../build src/index.ts src/verifier/index.ts --minify",
    "test": "jest",
    "pretest": "npm run build",
    "posttest": "npm run lint"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "node_modules/",
      "build/"
    ]
  }
}

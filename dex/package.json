{
  "name": "@pkmn/dex",
  "version": "0.7.13",
  "description": "A unification of Pok√©mon Showdown's client's and server's data layers",
  "repository": "github:pkmn/ps",
  "license": "MIT",
  "sideEffects": false,
  "main": "build/index.js",
  "module": "build/index.mjs",
  "types": "build/index.d.ts",
